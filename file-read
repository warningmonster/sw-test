#include <iostream>
#include <fstream>
using namespace std;

int Answer;
int N,M;
int input[40];

int main() {
	int T;
	int test_case,i;
	
	ifstream in("sample_input.txt");	
	in >> T;
 	cout << "T : " << T << endl;
 	 
	for(test_case = 1; test_case<= T; test_case++) {
		in >> N;
		printf("N : %d\n", N);
	    
		for(i=0; i<(N-1); i++)
		{
			in >> input[i];
			printf(" %d", input[i]);
		}
		cout << endl;
	}
	in.close();
 	return 0;
}
